    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.2",
    "axios": "^0.19.2",
    "for-editor": "^0.3.5",
    "highlight.js": "^10.1.1",
    "lint-staged": "^10.0.7",
    "mockjs": "^1.1.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-markdown": "^4.3.1",
    "redux": "^4.0.5",
    "umi": "^3.2.2",
    "yorkie": "^2.0.0"
  },
  "__npminstall_done": false
}
